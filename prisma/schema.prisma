generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model projects {
  id          Int     @id @default(autoincrement())
  title       String?
  description String?
}

model todos {
  id          Int       @id @default(autoincrement())
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  title       String?
  projectId   Int?
  status      String?   @default("todo")
  subtasks    Json[]
  description String?
}
